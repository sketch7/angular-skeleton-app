{
  "name": "angular-skeleton-app",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "npm run build:prod && npm run serve:prod",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve:prod": "cd ./dist/angular-skeleton-app && http-server",
    "clean": "rimraf dist",
    "analyze": "npm run build:prod  -- --stats-json && npm run analyze:only",
    "analyze:only": "npx webpack-bundle-analyzer ./angular-skeleton-app/dist/stats.json",
    "dev:ssr": "ng run angular-skeleton-app:serve-ssr",
    "serve:ssr": "node dist/angular-skeleton-app/server/main.js",
    "build:ssr": "ng build && ng run angular-skeleton-app:server",
    "prerender": "ng run angular-skeleton-app:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/platform-server": "^14.2.0",
    "@angular/router": "^14.2.0",
    "@angular/service-worker": "^14.2.0",
    "@nguniversal/express-engine": "^14.2.0",
    "@ssv/core": "^0.6.1",
    "@ssv/tools": "^0.9.1",
    "@types/lodash": "^4.14.157",
    "bootstrap": "^4.5.0",
    "express": "^4.15.2",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.21",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.7",
    "@angular-eslint/builder": "14.1.2",
    "@angular-eslint/eslint-plugin": "14.1.2",
    "@angular-eslint/eslint-plugin-template": "14.1.2",
    "@angular-eslint/schematics": "14.1.2",
    "@angular-eslint/template-parser": "14.1.2",
    "@angular/cli": "~14.2.7",
    "@angular/compiler-cli": "^14.2.0",
    "@nguniversal/builders": "^14.2.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.0.0",
    "@types/node": "^14.15.0",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "eslint": "^8.23.1",
    "eslint-plugin-ban": "^1.6.0",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-import": "^2.26.0",
    "http-server": "^0.12.3",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "rimraf": "^3.0.2",
    "typescript": "~4.7.2"
  }
}
